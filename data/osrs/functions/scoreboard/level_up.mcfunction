# Leveling celebrations
execute as @s run title @s times 5 60 5
execute as @s run title @s subtitle [{"text": "Level "}, {"score": {"name": "@s", "objective": "skill_level"}, "color": "gold"}]
execute as @s run playsound minecraft:ui.toast.challenge_complete master @s ~ ~ ~ 5 0.5
execute as @s run effect give @s minecraft:resistance 1 4 true
execute as @s run summon firework_rocket ~ ~2 ~ {LifeTime:15,FireworksItem:{id:firework_rocket,Count:1,tag:{Fireworks:{Flight:2,Explosions:[{Type:0,Flicker:0,Trail:1,Colors:[I;11743532,3887386,8073150,14602026]}]}}}}
execute as @s run summon firework_rocket ~-2 ~2 ~-3 {LifeTime:20,FireworksItem:{id:firework_rocket,Count:1,tag:{Fireworks:{Flight:2,Explosions:[{Type:0,Flicker:0,Trail:1,Colors:[I;11743532,3887386,8073150,14602026]}]}}}}

